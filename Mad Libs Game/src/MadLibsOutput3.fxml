<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.control.TextArea?>
<?import javafx.scene.image.Image?>
<?import javafx.scene.image.ImageView?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.text.Font?>

<AnchorPane prefHeight="600.0" prefWidth="600.0" style="-fx-background-color: Khaki;" xmlns="http://javafx.com/javafx/10.0.1" xmlns:fx="http://javafx.com/fxml/1" fx:controller="MadLibsOutput3Controller">
   <children>
      <Label fx:id="title" layoutX="267.0" layoutY="30.0" prefHeight="47.0" prefWidth="333.0" text="Our Class Project">
         <font>
            <Font name="Sitka Small" size="38.0" />
         </font>
      </Label>
      <ImageView fx:id="logo" fitHeight="60.0" fitWidth="60.0" layoutX="455.0" layoutY="7.0" pickOnBounds="true" preserveRatio="true">
         <image>
            <Image url="@MadLibsLogo.png" />
         </image>
      </ImageView>
      <TextArea fx:id="output" editable="false" layoutX="15.0" layoutY="73.0" prefHeight="299.0" prefWidth="570.0" wrapText="true">
         <font>
            <Font size="16.0" />
         </font>
      </TextArea>
      <Button fx:id="exit" layoutX="510.0" layoutY="515.0" mnemonicParsing="false" onAction="#closeProgram" onMouseExited="#play" onMouseMoved="#hovered" prefHeight="40.0" prefWidth="90.0" style="-fx-background-radius: 20; -fx-border-radius: 20; -fx-border-color: black; -fx-background-color: red;" text="EXIT" textFill="WHITE">
         <font>
            <Font name="System Bold" size="18.0" />
         </font>
      </Button>
      <Button fx:id="menu" layoutX="90.0" layoutY="515.0" mnemonicParsing="false" onAction="#openMainMenu" onMouseExited="#play" onMouseMoved="#hovered" prefHeight="40.0" prefWidth="115.0" style="-fx-background-radius: 20; -fx-border-radius: 20; -fx-border-color: black; -fx-background-color: white;" text="MAIN MENU">
         <font>
            <Font name="System Bold" size="15.0" />
         </font>
      </Button>
      <ImageView fitHeight="163.0" fitWidth="164.0" layoutX="220.0" layoutY="423.0" pickOnBounds="true" preserveRatio="true">
         <image>
            <Image url="@AnimalClipArt.png" />
         </image>
      </ImageView>
   </children>
</AnchorPane>
